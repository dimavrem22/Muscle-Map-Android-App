<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".ExerciseAnalysisFragment">

    <TextView
        android:id="@+id/woa_total_workouts_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:textColor="#FF332D2D"
        android:textSize="18dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView2"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        app:layout_constraintTop_toTopOf="@+id/textView2" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="15dp"
        android:text="Your Muscle Map"
        android:textColor="#000000"
        android:textSize="18dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/frontViewImage"
        android:layout_width="wrap_content"
        android:layout_height="456dp"
        android:layout_marginTop="30dp"
        android:src="@drawable/front_view"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />


    <ImageView
        android:id="@+id/bicepsFrontImage"
        android:layout_width="wrap_content"
        android:layout_height="456dp"
        android:layout_marginTop="30dp"
        android:src="@drawable/biceps_front"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <ImageView
        android:id="@+id/absFrontImage"
        android:layout_width="wrap_content"
        android:layout_height="456dp"
        android:layout_marginTop="30dp"
        android:src="@drawable/abs_front"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <ImageView
        android:id="@+id/trapsFrontImage"
        android:layout_width="wrap_content"
        android:layout_height="456dp"
        android:layout_marginTop="30dp"
        android:src="@drawable/traps_front"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />


    <ImageView
        android:id="@+id/pecsFrontImage"
        android:layout_width="wrap_content"
        android:layout_height="456dp"
        android:layout_marginTop="30dp"
        android:src="@drawable/peck_front"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <ImageView
        android:id="@+id/calvesFrontImage"
        android:layout_width="wrap_content"
        android:layout_height="456dp"
        android:layout_marginTop="30dp"
        android:src="@drawable/calves_front"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />


    <ImageView
        android:id="@+id/quadsFrontImage"
        android:layout_width="wrap_content"
        android:layout_height="456dp"
        android:layout_marginTop="30dp"
        android:src="@drawable/quads_front"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <ImageView
        android:id="@+id/forearmsFrontImage"
        android:layout_width="wrap_content"
        android:layout_height="456dp"
        android:layout_marginTop="30dp"
        android:src="@drawable/forearems_front"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <ImageView
        android:id="@+id/shouldersFrontImage"
        android:layout_width="wrap_content"
        android:layout_height="456dp"
        android:layout_marginTop="30dp"
        android:src="@drawable/shoulders_front"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <ImageView
        android:id="@+id/flipViewButton"
        android:layout_width="92dp"
        android:layout_height="95dp"
        android:layout_marginStart="10dp"
        android:src="@drawable/flip"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/frontViewImage"
        android:translationZ="90dp"/>

    <ImageView
        android:id="@+id/backViewImage"
        android:layout_width="wrap_content"
        android:layout_height="456dp"
        android:layout_marginTop="30dp"
        android:src="@drawable/back_view"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <ImageView
        android:id="@+id/glutesBackImage"
        android:layout_width="wrap_content"
        android:layout_height="456dp"
        android:layout_marginTop="30dp"
        android:src="@drawable/glutes_back"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <ImageView
        android:id="@+id/calvesBackImage"
        android:layout_width="wrap_content"
        android:layout_height="456dp"
        android:layout_marginTop="30dp"
        android:src="@drawable/calves_back"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <ImageView
        android:id="@+id/hamsBackImage"
        android:layout_width="wrap_content"
        android:layout_height="456dp"
        android:layout_marginTop="30dp"
        android:src="@drawable/hamstrings_back"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <ImageView
        android:id="@+id/forearmsBackImage"
        android:layout_width="wrap_content"
        android:layout_height="456dp"
        android:layout_marginTop="30dp"
        android:src="@drawable/foreams_back"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <ImageView
        android:id="@+id/lowerBackImage"
        android:layout_width="wrap_content"
        android:layout_height="456dp"
        android:layout_marginTop="30dp"
        android:src="@drawable/lower_back_back"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <ImageView
        android:id="@+id/latsBackImage"
        android:layout_width="wrap_content"
        android:layout_height="456dp"
        android:layout_marginTop="30dp"
        android:src="@drawable/lats_back"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <ImageView
        android:id="@+id/trapsBackImage"
        android:layout_width="wrap_content"
        android:layout_height="456dp"
        android:layout_marginTop="30dp"
        android:src="@drawable/traps_back"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <ImageView
        android:id="@+id/upperTrapsBackImage"
        android:layout_width="wrap_content"
        android:layout_height="456dp"
        android:layout_marginTop="30dp"
        android:src="@drawable/upper_traps_back"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <ImageView
        android:id="@+id/trisBackImage"
        android:layout_width="wrap_content"
        android:layout_height="456dp"
        android:layout_marginTop="30dp"
        android:src="@drawable/tris_back"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <ImageView
        android:id="@+id/shouldersBackImage"
        android:layout_width="wrap_content"
        android:layout_height="456dp"
        android:layout_marginTop="30dp"
        android:src="@drawable/shoulders_back"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <ProgressBar
        android:id="@+id/workoutAnalysisProgressBar"
        style="@android:style/Widget.DeviceDefault.Light.ProgressBar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/frontViewImage" />

    <Spinner
        android:id="@+id/exerciseAnalysisSpinner"
        style="@style/Widget.AppCompat.Spinner.DropDown"
        android:layout_width="wrap_content"
        android:layout_height="27dp"
        android:layout_marginEnd="5dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="30dp"
        android:text="Total Workouts: "
        android:textColor="#332D2D"
        android:textSize="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frontViewImage" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="6dp"
        android:text="Total Exercises: "
        android:textColor="#FF332D2D"
        android:textSize="16dp"
        app:layout_constraintStart_toStartOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/woa_total_exercises_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:textColor="#FF332D2D"
        android:textSize="18dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView5"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toTopOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="Total Workout Time: "
        android:textColor="#FF332D2D"
        android:textSize="16dp"
        app:layout_constraintStart_toStartOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/woa_total_time_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:textColor="#FF332D2D"
        android:textSize="18dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView8"
        app:layout_constraintStart_toEndOf="@+id/textView8"
        app:layout_constraintTop_toTopOf="@+id/textView8" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:text="Avearge Workout Time:"
        android:textColor="#FF332D2D"
        android:textSize="16dp"
        app:layout_constraintStart_toStartOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <TextView
        android:id="@+id/woa_avg_wo_time_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:textColor="#FF332D2D"
        android:textSize="18dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView10"
        app:layout_constraintStart_toEndOf="@+id/textView10"
        app:layout_constraintTop_toTopOf="@+id/textView10" />

    <ImageView
        android:id="@+id/woa_prev_button"
        android:layout_width="42dp"
        android:layout_height="42dp"
        android:layout_marginStart="20dp"
        android:src="@drawable/previous"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10" />


</androidx.constraintlayout.widget.ConstraintLayout>
